{
  "short_name": "Dashboard",
  "name": "Dashboard",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Personal new tab dashboard",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"]
}
